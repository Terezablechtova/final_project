        h2.landscape-page LANDSCAPES OF NEW ZEALAND
        div.row
                div#carouselExampleFade_1(class="carousel slide carousel-fade" data-ride="carousel")
                        div.carousel-inner
                        div.carousel-item.active
                                img.d-block.w-100(src='/img/mt_cook.jpg' alt='Mt Cook', class = "carousel_imgs")
                                div.carousel-caption.d-none.d-md-block
                                 h5.image-caption Mt Cook National Park
                        div.carousel-item
                                img.d-block.w-100(src='/img/tongariro.jpg' alt='Tongariro',  class = "carousel_imgs")
                                div.carousel-caption.d-none.d-md-block  
                                 h5 Tongariro National Park
                        div.carousel-item
                                img.d-block.w-100(src='/img/lake_tekapo.jpg' alt='Lake Tekapo', class = "carousel_imgs")
                                div.carousel-caption.d-none.d-md-block
                                 h5 Church of the Good Shepherd, Lake Tekapo
                        div.carousel-item
                                img.d-block.w-100(src='/img/tekapo_lake.jpg' alt='Lake Tekapo', class = "carousel_imgs")
                                div.carousel-caption.d-none.d-md-block
                                 h5 Lake Tekapo
                        a.carousel-control-prev(href='#carouselExampleFade_1' role="button" data-slide="prev")
                                span.carousel-control-prev-icon(aria-hidden='true')
                                span.sr-only Previous
                        a.carousel-control-next(href="#carouselExampleFade_1" role="button" data-slide="next")
                                span.carousel-control-next-icon(aria-hidden='true')
                                span.sr-only Next
        
