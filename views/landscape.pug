    body.landscape-page
      div.landscape-page.container
        
        div
                ul.nav.justify-content-center
                        li.nav-item
                        a.nav-link(href='/', class= 'landscape-page') HOME
                        li.nav-item
                        a.nav-link(href='/about', class= 'landscape-page') ABOUT
